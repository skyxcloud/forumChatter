{
    "name": "xcloud/forumchatter",
    "description": "A simple forum package for your laravel app with some modification for X-Cloud",
    "authors": [
        {
            "name": "X-Cloud",
            "email": "traian@x-cloud.biz"
        }
    ],
    "require": {
        "luketowers/purifier": "~3.0"
    },
    "require-dev": {
        "illuminate/cache": "~5.1.20|5.2.*|5.3.*",
        "illuminate/console": "~5.1.20|5.2.*|5.3.*",
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "~5.7.14"
    },
    "autoload": {
	    "psr-4": {
	      "xcloud\\ForumChatter\\": "/src"
		}
	},
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "xcloud\\ForumChatter\\ForumChatterServiceProvider"
            ]
        }
    }
}